{
  "name": "learn-algorithms",
  "version": "1.0.0",
  "description": "JavaScriptでいろいろなアルゴリズムを実装していくレポジトリ",
  "main": "index.js",
  "scripts": {
    "build": "webpack-cli --mode development",
    "doc": "esdoc",
    "watch": "yarn run build -- -w",
    "format:check": "prettier --check ./{src,test}/**/*.js",
    "format": "prettier --write ./{src,test}/**/*.js",
    "test": "ava src/**/*.spec.js",
    "cover": "nyc --reporter=lcov --reporter=text --report-dir=reports yarn test"
  },
  "keywords": [],
  "author": "rtamura",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.6",
    "@babel/register": "^7.28.3",
    "ava": "^3.15.0",
    "babel-loader": "^10.0.0",
    "esdoc": "^1.1.0",
    "nyc": "^17.1.0",
    "prettier": "^3.8.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^4.7.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
